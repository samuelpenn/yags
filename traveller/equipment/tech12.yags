            <strength>1</strength>
            <load>0.5</load>
            <reach>0</reach>

            <capacity>8</capacity>
            <rof>3</rof>
            <recoil>0</recoil>

            <properties>
                <missile/><semiauto/><laser/>
                <reload score="1"/>
            </properties>
        </weapon>
